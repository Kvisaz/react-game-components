{
  "name": "react-game-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://kvisaz.github.io/react-game-components",
  "scripts": {
    "vite": "vite",
    "build": "tsc -b && vite build",
